		if i '= 1{
			set predicate = predicate_" AND "
		}
		
		set predicate = predicate_"(Keywords [ '"_$LIST(queryParts, i)_"')"
		
		set i = i + 1
	}
	
	set sqlQuery = sqlQuery_" WHERE "_predicate
	
	//write sqlQuery
	
	do ##class(%ZEN.Auxiliary.jsonSQLProvider).%WriteJSONFromSQL(,sqlQuery)
]]></Implementation>
